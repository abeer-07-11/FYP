{
  "name": "cra-tailwindcss",
  "version": "0.1.0",
  "homepage": "./",
  "main": "./Electron/main.js",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.11.4",
    "@material-ui/data-grid": "^4.0.0-alpha.22",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.57",
    "axios": "^0.21.1",
    "clsx": "^1.1.1",
    "concurrently": "^6.2.0",
    "electron": "^13.1.8",
    "electron-builder": "^22.11.7",
    "electron-is-dev": "^2.0.0",
    "jason-server": "^3.0.0",
    "particles-bg": "^2.5.5",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "react-select": "^4.2.1",
    "react-slick": "^0.28.1",
    "react-spring-3d-carousel": "^1.2.1",
    "slick-carousel": "^1.8.1",
    "styled-components": "^5.2.1",
    "uuid": "^8.3.2",
    "wait-on": "^6.0.0"
  },
  "devDependencies": {
    "cypress": "6.6.0",
    "cypress-image-snapshot": "4.0.1",
    "eslint-plugin-cypress": "2.11.2",
    "start-server-and-test": "1.12.0",
    "tailwindcss": "1.9.6"
  },
  "scripts": {
    "prestart": "tailwind build ./src/index.tailwind.css -c ./tailwind.config.js -o ./src/index.css",
    "start": "react-scripts start",
    "json-server": "json-server --watch db.json --port 5000",
    "ebuild": "npm run build && node_modules/.bin/build",
    "start:dev": "concurrently \"npm start\"  \" npm run json-server\"  \"wait-on http://localhost:3000 && electron .\"",
    "prebuild": "tailwind build ./src/index.tailwind.css -c ./tailwind.config.js -o ./src/index.css",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "test": "cypress open",
    "test:ci": "start-server-and-test start http://localhost:3000 cy:run",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:updateSnapshots": "cypress run --env updateSnapshots=true"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
